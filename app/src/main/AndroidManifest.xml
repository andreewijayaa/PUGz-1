<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.pugz">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/logo3"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/logo3"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:debuggable="false">
        <activity android:name="pugzmain.example.pugz.Register">
        </activity>
        <activity android:name="pugzmain.example.pugz.Login">
        </activity>
        <activity android:name="pugzmain.example.pugz.AddGames">
        </activity>
        <activity android:name="pugzmain.example.pugz.Profile">
        </activity>
        <activity android:name="pugzmain.example.pugz.Portal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>